{
    "name": "pyaesoneaung/ssh-prompts",
    "description": "SSH account management cli tool developed with Laravel Prompts and Laravel Zero",
    "keywords": ["pyaesoneaung", "ssh", "laravel zero", "laravel prompts", "cli"],
    "homepage": "https://github.com/PyaeSoneAungRgn/ssh-prompts",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/PyaeSoneAungRgn/ssh-prompts/issues",
        "source": "https://github.com/PyaeSoneAungRgn/ssh-prompts"
    },
    "authors": [
        {
            "name": "Pyae Sone Aung",
            "email": "pyaesoneaungrgn@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laminas/laminas-text": "^2.5",
        "laravel/prompts": "^0.1.4",
        "nunomaduro/termwind": "^1.15.1"
    },
    "require-dev": {
        "laravel/pint": "^1.8",
        "mockery/mockery": "^1.5.1",
        "laravel-zero/framework": "^10.0.2",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/ssh-prompts"]
}
